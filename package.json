{
  "name": "duolingo-style-blog",
  "version": "1.0.0",
  "description": "A blog inspired by Duolingo's design style.",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "ts-node-dev --respawn src/app.ts",
    "build": "npm run build:next && npm run build:server && npm run build:client && npm run copy-json",
    "build:server": "tsc",
    "build:client": "webpack",
    "build:next": "next build",
    "dev:client": "webpack --watch",
    "dev:next": "next dev",
    "copy-json": "node scripts/copy-json.js",
    "stop": "./scripts/stop-blog-post",
    "test": "jest"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "http-server": "^14.1.1",
    "iron-session": "^8.0.4",
    "jsdom": "^26.0.0",
    "marked": "^15.0.8",
    "next": "^14.0.0",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/node": "^18.15.11",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "fs-extra": "^11.3.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4",
    "webpack": "^5.98.0",
    "webpack-cli": "^5.1.4"
  },
  "author": "Author Name",
  "license": "MIT",
